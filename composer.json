{
    "name": "bariew/yii2-cms-bootstrap",
    "description": "Attaches external modules to application",
    "type": "yii2-extension",
    "version": "0.2.0",
    "keywords": ["yii2","cms","bootstrap"],
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/bariew/yii2-cms-bootstrap",
    "time": "2014-05-27",
    "authors": [
        {
            "name": "Bariev Pavel",
            "email": "bariew@yandex.ru"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "bariew/yii2-module-migration-controller": "*@devr"
    },
    "autoload": {
        "psr-4": {
            "bariew\\cmsBootstrap\\": ""
        }
    },
    "extra": {
        "bootstrap": "bariew\\cmsBootstrap\\CmsBootstrap"
    }    
}
