{
    "name": "bariew/yii2-cms-bootstrap",
    "description": "Attaches external modules to application",
    "type": "yii2-extension",
    "keywords": ["yii2","cms","bootstrap"],
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/bariew/yii2-cms-bootstrap",
    "time": "2014-05-27",
    "minimum-stability" : "dev",
    "authors": [
        {
            "name": "Bariev Pavel",
            "email": "bariew@yandex.ru"
        }
    ],
    "require": {
        "php"           : ">=5.4.0",
        "yiisoft/yii2"  : "*",
        "bariew/yii2-module-migration-controller"   : "1.*",
        "bariew/yii2-event-component"               : "1.*"
    },
    "autoload": {
        "psr-4": {
            "bariew\\cmsBootstrap\\": ""
        }
    },
    "extra": {
        "bootstrap": "bariew\\cmsBootstrap\\CmsBootstrap"
    }    
}
